{
    "name": "petertilsen/bpmnbundle",
    "type": "symfony-bundle",
    "description": "Bpmn gateway",
    "keywords": ["api", "rest", "bpmn"],
    "homepage": "http://glassesgroupglobal.com",
    "license": "MIT",
    "authors": [
        {
            "name":     "Peter Tilsen",
            "homepage": "http://glassesgroupglobal.com"
        },
        {
            "name":     "GGG",
            "homepage": "http://glassesgroupglobal.org"
        }
    ],
    "require": {
        "php":                      ">=5.3.3",
        "friendsofsymfony/rest-bundle":         "~1.0@dev",
        "nelmio/api-doc-bundle":                "2.7.x-dev"
    },
    "require-dev": {
        "phpspec/phpspec": "~2.0",
        "phpunit/phpunit": "~3.7"
    },
    "config": {
        "bin-dir": "bin",
        "vendor-dir": "vendor/"
    },

    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    }
}
